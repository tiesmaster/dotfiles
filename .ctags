--langdef=magik
--langmap=magik:.magik
--regex-magik=/_method[ \t]*([a-zA-Z0-9_]+).([a-zA-Z0-9_]+\??)/\2/d,definition/
--regex-magik=/def_slotted_exemplar[ \t]*\([ \t]*:([a-zA-Z0-9_]+)/\1/d,definition/
--regex-magik=/_global[ \t]*([a-zA-Z0-9_]+)/\1/d,definition/
--regex-magik=/_global[ \t]*_constant[ \t]*([a-zA-Z0-9_]+)/\1/d,definition/
